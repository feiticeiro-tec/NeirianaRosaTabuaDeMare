<Row>:
    orientation:'vertical'
    adaptive_height:True
    spacing:dp(1)
    padding:dp(0),dp(25),dp(0),dp(0)
    MDBoxLayout:
        size_hint_y:None
        height:dp(20)
        MDLabel:
            text:root.text_1
            color:142/255,105/255,78/255,1
            halign:'center'
            font_size:dp(30)
            font_name:'./NeirianaRosa/font/buttershine.otf'
        MDLabel:
            text:root.text_2
            color:142/255,105/255,78/255,1
            halign:'center'
            font_size:dp(30)
            font_name:'./NeirianaRosa/font/buttershine.otf'
    MDBoxLayout:
        size_hint:0.9,None
        pos_hint:{'center_x':0.5}
        height:dp(1)
        md_bg_color:142/255,105/255,78/255,1
    MDBoxLayout:
        size_hint:0.85,None
        pos_hint:{'center_x':0.5}
        height:dp(1)
        md_bg_color:142/255,105/255,78/255,1

